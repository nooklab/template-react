    "babel-loader": "^8.1.0",
    "craco-alias": "^2.1.1",
    "cypress": "^8.5.0",
    "cypress-file-upload": "^5.0.8",
    "dotenv-cli": "^4.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "firebase-admin": "^9.12.0",
    "styled-system": "^5.1.5"
  }
