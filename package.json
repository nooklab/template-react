  "scripts": {
    "start": "npm run start:local",
    "start:local": "GENERATE_SOURCEMAP=false dotenv -e env/.env -e env/.env.local craco start",
    "start:dev": "export PORT=4003 && dotenv -e env/.env -e env/.env.dev craco start",
    "start:prod": "dotenv -e env/.env -e env/.env.prod craco start",
    "build": "craco build",
    "build:local": "dotenv -e env/.env -e env/.env.local craco build",
    "build:dev": "dotenv -e env/.env -e env/.env.dev craco build",
    "build:staging": "dotenv -e env/.env -e env/.env.staging craco build",
    "build:prod": "rm -rf ./build && dotenv -e env/.env -e env/.env.prod craco build",
    "build:ver": "REACTBUILDVERSION=$(date +%Y-%m-%d)-$(date +%T) && sed -i -- 's/%REACTBUILDVERSION%/'$REACTBUILDVERSION'/g' build/index.html && echo React Build Version = REACTBUILDVERSION",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "upload:dev": "aws s3 sync ./build s3://qyours-front-dev --profile bloglink",
    "publish:dev": "yarn build:dev && yarn upload:dev && yarn invalidateCache:dev",
    "invalidateCache:dev": "aws cloudfront create-invalidation --distribution-id E23VMNFYFOURPG --paths '/*' --profile bloglink",
    "cypress": "cypress open",
    "cypress-run": "cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
